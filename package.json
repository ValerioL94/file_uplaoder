  "dependencies": {
    "@prisma/client": "^5.18.0",
    "@quixo3/prisma-session-store": "^3.1.13",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.4.0",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "express-ejs-layouts": "^2.5.1",
