<%- contentFor('page') %>

<div class="gen-container">
  <% if (locals.user) { %>
  <div>
    <ul>
      <li>
        File name:
        <%= file.title %>
      </li>
      <li>
        File size:
        <%= file.size %>
        bytes
      </li>
      <li>
        File url:
        <a href="<%= file.url %>" style="overflow-wrap: anywhere"
          ><%= file.url %></a
        >
      </li>
      <li>
        Upload time:
        <%= file.createdAt.toLocaleString() %>
      </li>
      <li>
        Download: <a href="<%= file.id %>/download">link</a>
      </li>
    </ul>
  </div>
  <% } else { %>
  <p>Access denied</p>
  <% } %>
  <%- include('./partials/back-link') %>
</div>
